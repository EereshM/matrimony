{
  "entrypoint": "index.html",
  "shell": "src/honeymoney-app-app/honeymoney-app-app.js",
  "fragments": [
    
    "src/honeymoney-app-app/home-page.js",
    "src/honeymoney-app-app/login-form.js",
    "src/honeymoney-app-app/my-matches.js",
    "src/honeymoney-app-app/my-profile.js",
    "src/honeymoney-app-app/my-view404.js",
    "src/honeymoney-app-app/notification-page.js",
    "src/honeymoney-app-app/registration-form.js",
    "src/honeymoney-app-app/request-page.js",
    "src/honeymoney-app-app/regPage1.js",
    "src/honeymoney-app-app/regPage2.js",
    "src/honeymoney-app-app/regPage3.js"
  ],
  "sources": [
    "src/**/*",
    "manifest/**",
    "package.json"
   ],
   "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "moduleResolution": "node",
  "builds": [
    {
      "preset": "es5-bundled",
      "addServiceWorker": true
    },
    {
      "preset": "es6-unbundled",
      "addServiceWorker": true
    },
    {
      "preset": "es6-bundled",
      "addServiceWorker": true
    }
  ],

  "npm": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  }
}
